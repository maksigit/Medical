                +e.list__wrap
                  +e.A.title(href='') Товары для дома
                  +e.UL.list
                    for name in catalog2
                      +e.LI.item
                        +e.A.link(href='')= name.link1
                  +e.open__list(href='') Показать еще 15 категорий
                  +e.UL.hide__list
                    for name in hideList1
                      +e.LI.item
                        +e.A.link(href='')= name.link1
            .col-1
              +b.catalog
                +e.img
                  img(src='images/img3.png', alt='')
                +e.list__wrap
                  +e.A.title(href='') Для здоровья и красоты
                  +e.UL.list
                    for name in catalog3
                      +e.LI.item
                        +e.A.link(href='')= name.link1
            .col-1
              +b.catalog
                +e.img
                  img(src='images/img4.png', alt='')
                +e.list__wrap
                  +e.A.title(href='') Мебель медицинская
                  +e.UL.list
                    for name in catalog4
                      +e.LI.item
                        +e.A.link(href='')= name.link1
                  +e.open__list(href='') Показать еще 5 категорий
                  +e.UL.hide__list
                    for name in hideList2
                      +e.LI.item
                        +e.A.link(href='')= name.link1
        +b.btn
          +e.A.catalog(href='') перейти в полный каталог продукции
          +e.desc Еще 9 категорий медицинских товаров.
    footer

    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
    script(src='scripts/mobile-menu.min.js')
    script(src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.1.6/js/swiper.min.js")
